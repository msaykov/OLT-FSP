@model AddTargetFormModel
@{
    ViewData["Title"] = "Add Target";
}

<div class="container">
    <div class="text-center">
        <h1 class="display-4">@ViewData["Title"]</h1>

    </div>

    <form method="post">
        <div class="form-group">
            <label asp-for="Destination"></label>
            <input asp-for="Destination" class="form-control" id="validationTooltip01" placeholder="cabinet Mladost-3 , bl.324 ...">
            <span asp-validation-for="Destination" class="small text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="CoremapNumber"></label>
            <input asp-for="CoremapNumber" class="form-control" id="validationTooltip01" placeholder="Coremap Id...">
            <span asp-validation-for="CoremapNumber" class="small text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="Zone"></label>
            <input asp-for="Zone" class="form-control" id="validationTooltip01" placeholder="Mladost, Lulin ...">
            <span asp-validation-for="Zone" class="small text-danger"></span>
        </div>        
        <input class="btn btn-primary" type="submit" value="Save" />
    </form>

</div>